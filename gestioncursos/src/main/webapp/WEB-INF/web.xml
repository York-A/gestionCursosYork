    <servlet-class>com.ipartek.formacion.controller.AlumnoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AlumnoServlet</servlet-name>
    <url-pattern>/alumnos.do</url-pattern>
  </servlet-mapping>
